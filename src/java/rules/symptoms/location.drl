import droolsEvaluation.Where

rule "indoor"

    dialect "java"
    when
        w1 : Where (place.equals("Indoors"))
    then 
        w1.setVehicule(Where.Vehicule.AMBULANCE);
        System.out.println("Rule indoor applied over " + w1);
        System.out.println("Vehicle display indoors is " + w1);
end




