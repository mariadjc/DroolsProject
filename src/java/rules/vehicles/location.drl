import droolsEvaluation.Location

rule "indoor"

    dialect "java"
    when
        w1 : Location (place.equals("Indoors"))
    then 
        w1.setVehicle(Location.Vehicle.AMBULANCE);
        System.out.println("Rule indoor applied over " + w1);
        System.out.println("Vehicle display indoors is " + w1);
end




